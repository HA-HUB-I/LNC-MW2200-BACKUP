WAIT[0,1];

#101=LEN_STD_R[0,0,9000];/* 讀取Z軸安全高度座標 */
#102=R_REG[9427];        /* 主軸2下M碼 */     

G53 Z#101;               /* Z軸回到安全點 */
M#102;                   /* 主軸2下 */

M99;
